<div class="config-item">
    <span class="config-label">{{ label }}:</span>
    {{if items && items.size > 0}}
    <div class="config-list">
        {{for item in items}}
        <span class="config-list-item">{{ item }}</span>
        {{end}}
    </div>
    {{else}}
    <span class="config-value config-empty">(None)</span>
    {{end}}
</div>
